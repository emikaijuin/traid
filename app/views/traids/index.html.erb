<p id="notice"><%= notice %></p>

<h1>Listing Traids</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @traids.each do |traid| %>
      <tr>
        <td><%= link_to 'Show', traid %></td>
        <td><%= link_to 'Edit', edit_traid_path(traid) %></td>
        <td><%= link_to 'Destroy', traid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Traid', new_traid_path %>
